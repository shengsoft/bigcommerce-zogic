{"components/blog/post":"<article class=\"blog\">\r\n    <div class=\"blog-post-body\">\r\n        <header class=\"blog-header\">\r\n            <h2 class=\"blog-title\">\r\n                <a href=\"{{post.url}}\">{{post.title}}</a>\r\n            </h2>\r\n            <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\r\n        </header>\r\n\r\n        <div class=\"blog-post-figure\">\r\n            {{#if post.thumbnail}}\r\n                <figure class=\"blog-thumbnail\">\r\n                    <a href=\"{{post.url}}\">\r\n                        <img src=\"{{getImage post.thumbnail 'blog_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\r\n                    </a>\r\n                </figure>\r\n            {{/if}}\r\n        </div>\r\n\r\n        <div class=\"blog-post\">\r\n            {{#if post.body}}\r\n                {{{post.body}}}\r\n            {{else}}\r\n                {{post.summary}}\r\n            {{/if}}\r\n        </div>\r\n\r\n        {{#if post.show_read_more}}\r\n            <div class=\"clear\"></div>\r\n            <a class=\"blog-readmore btn btn-default\" href=\"{{url}}\">Read more</a>\r\n        {{/if}}\r\n\r\n        {{#if show_tags}}\r\n            {{#if post.tags}}\r\n                <ul class=\"tags\">\r\n                    {{#each post.tags}}\r\n                    <li class=\"tag\">\r\n                        <a href=\"{{url}}\">{{name}}</a>\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n            {{/if}}\r\n        {{/if}}\r\n\r\n        {{>components/common/share}}\r\n\r\n    </div>\r\n</article>\r\n","components/common/share":"<div class=\"social-mediaBlock\">\r\n    <script type=\"text/javascript\" src=\"//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-564d3080dd24d74d\" async=\"async\"></script>\r\n    <!-- Go to www.addthis.com/dashboard to customize your tools -->\r\n    <div class=\"addthis_native_toolbox\"></div>\r\n</div>\n"}
