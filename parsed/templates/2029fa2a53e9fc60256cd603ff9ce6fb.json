{"components/account/wishlist-list":"<table class=\"table table--line wishlists-table\">\r\n    <thead class=\"table-thead\">\r\n        <tr>\r\n            <th>{{lang 'account.wishlists.name'}}</th>\r\n            <th>{{lang 'account.wishlists.num_items'}}</th>\r\n            <th>{{lang 'account.wishlists.shared'}}</th>\r\n            <th>{{lang 'account.wishlists.action'}}</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody class=\"table-tbody\">\r\n    {{#each customer.wishlists}}\r\n        <tr>\r\n            <td><a href=\"{{view_url}}\">{{name}}</a></td>\r\n            <td>{{num_items}}</td>\r\n            <td>\r\n                {{#if is_public }}\r\n                    {{lang 'common.yes'}}\r\n                {{else}}\r\n                    {{lang 'common.no'}}\r\n                {{/if}}\r\n            </td>\r\n            <td class=\"table-actions\">\r\n                <form method=\"post\" action=\"{{delete_url}}\" class=\"form\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                        {{#if is_public}}\r\n                            <a href=\"{{share_url}}\" class=\"btn btn-primary btn-sm\"\r\n                           role=\"button\">{{lang 'common.share'}}</a>\r\n                        {{/if}}\r\n                        <a href=\"{{edit_url}}\" class=\"btn btn-alt btn-sm\" data-wishlist>{{lang 'common.edit'}}</a>\r\n                        <input type=\"submit\" value=\"{{lang 'common.delete'}}\" class=\"btn btn-oranges btn-sm\" data-wishlist-delete>\r\n                    </fieldset>\r\n                </form>\r\n            </td>\r\n        </tr>\r\n    {{/each}}\r\n    </tbody>\r\n</table>\r\n<form method=\"post\" action=\"{{this.urls.account.wishlists.delete}}\" class=\"form\">\r\n    {{#unless customer.wishlists}}\r\n        <p class=\"form-actions\">{{> components/common/alert-info (lang 'account.wishlists.you_have_none')}}</p>\r\n\r\n    {{/unless}}\r\n    <div class=\"form-actions\">\r\n        <a href=\"{{this.urls.account.wishlists.add}}\" class=\"btn btn-primary\" role=\"button\" data-wishlist>{{lang 'account.wishlists.new'}}</a>\r\n        {{inject 'wishlistDelete' (lang 'account.wishlists.delete_alert')}}\r\n        {{#if customer.wishlists}}\r\n            {{#each customer.wishlists}}\r\n                <input type=\"hidden\" value=\"{{this.id}}\" name=\"deletewishlist[]\">\r\n            {{/each}}\r\n            <input type=\"submit\" value=\"{{lang 'account.wishlists.delete_all'}}\" class=\"btn btn-oranges\" data-wishlist-delete>\r\n        {{/if}}\r\n    </div>\r\n</form>\r\n<script>\r\nwindow.onload=function() {\r\n    var m,\r\n        productIdMap = {},\r\n        productIdsOnPage = getUniqueProductIdsOnPage(),\r\n        urlParams = decodeURIComponent(window.location.search);\r\n    fbq('track', 'AddToWishlist', { content_ids: [m[1]], content_type: 'product_group' });\r\n}\r\n</script>\n","components/common/alert-info":"<div class=\"alert alert-info\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n"}
